source ~/dotfiles/.vimrc_synced


curl -fLo ~/.vim/autoload/plug.vim --create-dirs \
    https://raw.githubusercontent.com/junegunn/vim-plug/master/plug.vim




space r				reload .vimrc
:Maketags

CTRL ^              switch to last buffer

space n				nerdtree focus
space t				nerdtree toggle

SHIFT tab
CTRL w w			split switch
					
CTRL w r			rotate windows around

CTRL HJKL			split navigation

CTRL ]
CTRL I
CTRL O
CTRL T				stack jump shaite



NEOVIM:

space d             show diagnostic

TELESCOPE:
vim.keymap.set('n', '<space>ff', ':Telescope find_files<cr>')
vim.keymap.set('n', '<space>fg', ':Telescope git_files<cr>')
vim.keymap.set('n', '<space>fr', ':Telescope live_grep<cr>')
vim.keymap.set('n', '<space>fb', ':Telescope buffers<cr>')



NEOTREE:
vim.keymap.set('n', '<tab>', ':Neotree toggle<cr>')
vim.keymap.set('n', '<space><tab>', ':Neotree toggle current reveal_force_cwd<cr>')




./ctags_script.sh fuckery.c

